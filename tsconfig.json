{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "strictNullChecks": true,
    "baseUrl": "E:/DevReact/alp",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@content/*": [
        "./src/content/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@icons/*": [
        "./src/components/icons/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}